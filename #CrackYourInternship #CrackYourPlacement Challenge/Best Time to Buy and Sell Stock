class Solution {
public:
    int maxProfit(vector<int>& prices) {
        
         int maxi = 0;

            int sub = prices[0];

          
            for(int i = 1;i<prices.size();i++){

                if(sub > prices[i]){
                    sub = prices[i];
                }else{
                    maxi = max(maxi,prices[i] - sub);
                }
            }
            return maxi;
            
    }
};
