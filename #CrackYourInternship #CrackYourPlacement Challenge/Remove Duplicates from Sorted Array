 int removeDuplicates(vector<int>& nums) {
        
        int i = 0;
        int j = i+1;

        while(i<nums.size() and j<nums.size()){

            if(nums[i] != nums[j]){
                i++;
                j++;
            }else{
                nums.erase(nums.begin() + j); 
            }
        }

        return nums.size();
    }
