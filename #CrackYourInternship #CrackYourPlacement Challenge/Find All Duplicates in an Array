class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        
        int size =*max_element(nums.begin(),nums.end());

        vector<int>ans(size+1,0);

        for(int i=0;i<nums.size();i++){
            ans[nums[i]]++;
        }

        vector<int>res;

        for(int i=0;i<ans.size();i++){
            if(ans[i] >= 2){
                res.push_back(i);
            }
        }
        return res;

    }
};
